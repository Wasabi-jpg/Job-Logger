{
  "name": "Job Logger Chatbot",
  "description": "Logs jobs so I don't have to switch so many tabs.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "background": {
    "service_worker": "background.js", 
    "type": "module" 
  },
  "permissions": [
    "storage" 
  ],
  "host_permissions": [ 
    "https://*.amazonaws.com/" 
  ]

}